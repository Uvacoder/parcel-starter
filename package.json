{
	"name": "parcel-starter",
	"version": "1.0.0",
	"main": "src/index.html",
	"license": "MIT",
	"scripts": {
		"start": "parcel src/index.html -p 3000 --open",
		"build:parcel": "parcel build src/index.html",
		"build:tailwind": "npx tailwind build src/scss/styles.scss -o dist/styles.css --no-autoprefixer",
		"build": "npm run clean && npm run build:parcel",
		"clean": "rm -rf dist/*"
	},
	"dependencies": {},
	"devDependencies": {
		"@babel/core": "^7.6.4",
		"@babel/preset-env": "^7.6.3",
		"node-sass": "^4.12.0",
		"parcel-bundler": "^1.12.4",
		"parcel-plugin-imagemin": "^3.0.0",
		"parcel-plugin-svg-sprite": "^1.4.1",
		"postcss": "^7.0.21",
		"postcss-import": "^12.0.1",
		"tailwindcss": "^1.1.3"
	}
}
