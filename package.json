{
	"name": "parcel-starter",
	"version": "1.0.0",
	"main": "index.html",
	"license": "MIT",
	"scripts": {
		"start": "run-p parcel scss",
		"parcel": "parcel src/index.html -p 3000 --open",
		"build:parcel": "parcel build src/index.html",
		"build": "npm run clean && npm run build:parcel",
		"scss": "node-sass --output-style compressed -o dist/css src/scss",
		"clean": "rm -rf dist/*"
	},
	"dependencies": {},
	"devDependencies": {
		"node-sass": "^4.12.0",
		"npm-run-all": "^4.1.5",
		"parcel-bundler": "^1.12.4",
		"parcel-plugin-imagemin": "^3.0.0",
		"parcel-plugin-svg-sprite": "^1.4.1"
	}
}
